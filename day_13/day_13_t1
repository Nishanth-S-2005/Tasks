let arr=['Apple','Orange','Grapes','guava','Banana'];
console.log(arr[2]);
arr.push('Mango');
console.log(arr);
arr.shift();
console.log(arr);
console.log(arr.length);
for(let i=0;i<arr.length;i++)
    console.log(arr[i]);
arr.reverse();
console.log(arr);
let arr1=[1,2,3,4,5];
var ans=0;
for(let i=0;i<5;i++)
    ans+=arr1[i];
console.log(ans);
    